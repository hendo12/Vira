<h2>Top Stories</h2>

<div class="story-row jumbotron row">
  {{#each storiesToHBS}}
  <div class="story-card col-md-4">
    <a href="story/{{_id}}" class="card-title">
      <h3>{{title}}</h3>
    </a>
    <img src="{{image}}" >
  </div>

  {{else}}
  <p>There are no posts to show right now!</p>
  {{/each}}
</div>

